<?xml version="1.0" encoding="windows-1252"?>
<shortcuts>
    <skipIfNotSupported/>
    <programGroup defaultName="Raven Studio"
        location="applications"/>

    <shortcut
        name="Raven Studio"
        programGroup="yes"
        desktop="yes"
        applications="no"
        startMenu="no"
        startup="no"
        target="$INSTALL_PATH\bin\ravenStudio.sh"
        commandLine=""
        workingDirectory="$USER_HOME"
        description="Raven Studio - create and animate!"
        iconFile="$INSTALL_PATH\ravenCameo64.png"
        iconIndex="0"
        initialState="normal"
        >
    </shortcut>

    <shortcut
        name="Uninstaller"
        programGroup="yes"
        desktop="no"
        applications="no"
        startMenu="no"
        startup="no"
        target="$INSTALL_PATH\Uninstaller\uninstaller.jar"
        commandLine=""
        description="Raven Studio uninstaller"
        >
    </shortcut>
</shortcuts>
